{
  "name": "powerweb",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon -L ./bin/www",
    "debug": "DEBUG=f_contact* nodemon -L ./bin/www",
    "eslint": "eslint ."
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4002
    }
  },
  "dependencies": {
    "client-sessions": "^0.8.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "idb-pconnector": "^1.0.8",
    "itoolkit": "^0.1.6",
    "morgan": "~1.9.1",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0"
  }
}
